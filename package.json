{
	"name": "reactsetup",
	"version": "1.1.0",
	"private": true,
	"author": "Gustavo Matheus <gustavo.morinaga27@gmail.com>",
	"description": "Template for React, with Next.js, SWR, Axios, TypeScript, ESLint, Prettier, Framer Motion, Tailwind and Sass",
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		},
		"lint-staged": {
			"*.{js,jsx,ts,tsx}": "yarn run lint:fix"
		}
	},
	"scripts": {
		"dev": "next dev",
		"lint": "next lint",
		"lint:fix": "eslint --cache --fix \"src/**/*.{js,jsx,ts,tsx}\"",
		"prepare": "husky install",
		"pre-commit": "yarn lint",
		"build": "next build",
		"start": "next start"
	},
	"dependencies": {
		"axios": "^0.24.0",
		"framer-motion": "^4.1.17",
		"next": "^12.0.0",
		"next-compose-plugins": "^2.2.1",
		"next-images": "^1.8.1",
		"next-pwa": "^5.4.0",
		"next-seo": "^4.28.1",
		"postcss-import": "^14.0.2",
		"postcss-nesting": "^8.0.1",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-icons": "^4.3.1",
		"sass": "^1.43.4",
		"sharp": "^0.29.2",
		"swr": "^1.0.1"
	},
	"devDependencies": {
		"@commitlint/cli": "^13.2.1",
		"@commitlint/config-conventional": "^13.2.0",
		"@next/eslint-plugin-next": "^12.0.0",
		"@types/node": "^16.11.6",
		"@types/react": "^17.0.33",
		"@typescript-eslint/eslint-plugin": "^5.2.0",
		"@typescript-eslint/parser": "^5.2.0",
		"autoprefixer": "^10.3.7",
		"eslint": "^7.32.0",
		"eslint-config-next": "^12.0.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^4.0.0",
		"husky": "^7.0.4",
		"lint-staged": "^11.2.6",
		"postcss": "^8.3.11",
		"prettier": "^2.4.1",
		"tailwindcss": "^2.2.17",
		"typescript": "^4.4.4"
	}
}
