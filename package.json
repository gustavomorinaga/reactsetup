{
  "name": "reactsetup",
  "version": "1.1.0",
  "private": true,
  "author": "Gustavo Matheus <gustavo.morinaga27@gmail.com>",
  "description": "Template for React, with Next.js, SWR, Axios, TypeScript, ESLint, Prettier, Framer Motion, Tailwind and Sass",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    },
    "lint-staged": {
      "*.{js,jsx,ts,tsx}": "yarn run lint:fix"
    }
  },
  "scripts": {
    "dev": "next dev",
    "lint": "next lint",
    "lint:fix": "eslint --cache --fix \"src/**/*.{js,jsx,ts,tsx}\"",
    "prepare": "husky install",
    "pre-commit": "yarn lint",
    "build": "next build",
    "start": "next start"
  },
  "dependencies": {
    "axios": "^0.21.4",
    "framer-motion": "^4.1.17",
    "next": "^11.1.2",
    "next-compose-plugins": "^2.2.1",
    "next-images": "^1.8.1",
    "next-pwa": "^5.3.1",
    "next-seo": "^4.28.1",
    "postcss-import": "^14.0.2",
    "postcss-nesting": "^8.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0",
    "sass": "^1.39.2",
    "sharp": "^0.29.1",
    "swr": "^1.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@next/eslint-plugin-next": "^11.1.2",
    "@types/node": "^16.9.1",
    "@types/react": "^17.0.20",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "autoprefixer": "^10.3.4",
    "babel-plugin-inline-react-svg": "^2.0.1",
    "eslint": "^7.32.0",
    "eslint-config-next": "^11.1.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "postcss": "^8.3.6",
    "prettier": "^2.4.0",
    "tailwindcss": "^2.2.15",
    "typescript": "^4.4.3"
  }
}
